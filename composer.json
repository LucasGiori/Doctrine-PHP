{
    "name": "lucas/apiprocesscontrol",
    "authors": [
        {
            "name": "Lucas Giori Cesconetto",
            "role": "Developer"
        }
    ],
    "description": "API with slim framework",
    "keywords": ["framework", "slim", "api", "doctrine"],
    "type": "project",
    "require": {
        "slim/slim": "^4.7",
        "slim/psr7": "^1.3",       
        "php-di/slim-bridge": "^3.0",
        "doctrine/orm": "^2.7",
        "vlucas/phpdotenv": "^5.0",
        "symfony/dotenv": "^5.2",
        "ext-json": "*",
        "symfony/serializer": "^5.2",
        "symfony/property-access": "^5.2",
        "doctrine/annotations": "^1.11",
        "symfony/validator": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
